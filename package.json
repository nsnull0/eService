{
    "name": "eservice",
    "version": "0.0.5",
    "description": "eservice",
    "main": "./app",
    "ava": {
        "files": [
            "test/*.js",
            "**/*.test.js"
        ],
        "concurrency": 5,
        "failFast": false,
        "failWithoutAssertions": false,
        "powerAssert": true,
        "tap": false
    },
    "eslintIgnore": [
        "coverage/*",
        "jsdoc/*"
    ],
    "scripts": {
        "prestart": "set NODE_ENV=production && npm run lint && npm run jsdoc",
        "start": "node --experimental-modules ./app",
        "test": "set NODE_ENV=test && nyc ava --verbose",
        "test:lint": "npm run lint && npm test",
        "test:watch": "npm test --watch",
        "jsdoc:prune": "rimraf ./jsdoc/",
        "jsdoc": "npm run jsdoc:prune && jsdoc --verbose -c .jsdoc.json",
        "lint": "eslint . --ext .js,.jsx -f table",
        "serve:jsdoc": "npm run jsdoc && serve ./jsdoc/ && npm start",
        "watch": "nodemon --exec npm start"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nsnull0/eService.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nsnull0/eService/issues"
    },
    "homepage": "https://github.com/nsnull0/eService#readme",
    "dependencies": {
        "body-parser": "^1.18.2",
        "cluster": "^0.7.7",
        "compression": "^1.7.1",
        "cors": "^2.8.4",
        "csurf": "^1.9.0",
        "dotenv": "^5.0.0",
        "express": "^4.16.2",
        "express-rate-limit": "^2.11.0",
        "get-port": "^3.2.0",
        "helmet": "^3.10.0",
        "lodash": "^4.17.4",
        "mongodb": "^3.0.2",
        "morgan": "^1.9.0",
        "passport": "^0.4.0",
        "pidusage": "^1.2.0",
        "response-time": "^2.3.2"
    },
    "devDependencies": {
        "@gunawanwijaya/minami": "^2.0.0",
        "@vue/cli": "^3.0.0-alpha.6",
        "ava": "^0.25.0",
        "eslint": "^4.16.0",
        "jsdoc": "^3.5.5",
        "nyc": "^11.4.1",
        "rimraf": "^2.6.2",
        "serve": "^6.4.9"
    }
}
