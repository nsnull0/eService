{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./", "README.md"],
    "exclude": ["./node_modules/"],
    "includePattern": "(.js$)",
    "excludePattern": "(node_modules/)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./.jsdoc/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/@gunawanwijaya/minami"
  }
}
